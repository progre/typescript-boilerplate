{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "es2017",
      "dom",
      "dom.iterable"
    ],
    "module": "commonjs",
    "noUnusedLocals": true,
    "plugins": [
      {
        "name": "typescript-tslint-plugin"
      }
    ],
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ]
}